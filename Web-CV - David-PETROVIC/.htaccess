#HTTPS Redirection
RewriteEngine on
RewriteCond %{SERVER_PORT} 80
RewriteRule .* https://%{HTTP_HOST}%{REQUEST_URI} [R=301,L]

#Deny htaccess
<Files .htaccess>
Order allow,deny
Deny from all
</Files>

<FilesMatch "^\.(off)">
    Order allow,deny
    Deny from all
</FilesMatch>

Options -Indexes

RedirectPermanent js  https://petrovic-david.fr